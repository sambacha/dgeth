{
  "name": "dgeth",
  "version": "0.0.1",
  "private": true,
  "description": "Go-Ethereum Backing Provider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sambacha/dgeth.git"
  },
  "keywords": [],
